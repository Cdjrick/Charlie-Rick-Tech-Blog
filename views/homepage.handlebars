{{#each posts}}

<ol class="post-list">
  <li>
    <article class="post">
      <div class="title">
      </div>
      <div class="meta">
        {{vote_count}} point(s) by {{user.username}} on
        {{dateFormat created_at}}
        |
        <a href="/post/{{id}}">{{comments.length}} comment(s)</a>
      </div>
    </article>
  </li>
</ol>

{{/each}}